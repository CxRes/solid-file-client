{
  "scripts": {
    "test": "jest --coverage",
    "clean": "rimraf dist coverage",
    "prepublish": "npm run test && npm run build",
    "watch": "babel --watch src --out-dir js",
    "build": "npm run clean && npm run build:lib && npm run build:module && npm run build:window",
    "build:lib": "rm -rf dist && babel src --env-name production -d dist",
    "build:module": "rm -rf dist/esm && babel src --env-name module -d dist/esm",
    "build:window": "webpack --config=./webpack/webpack.window.config.js -p"
  },
  "main": "dist/index.js",
  "browser": "dist/umd/solid-file-client.bundle.js",
  "module": "dist/esm/index.js",
  "name": "solid-file-client",
  "version": "0.2.0",
  "description": "A library for creating and managing files and folders in Solid data stores",
  "keywords": [
    "Solid",
    "Linked Data"
  ],
  "author": "Jeff Zucker <dubzed@gmail.com>",
  "license": "MIT",
  "browserslist": "> 0.25%, not dead",
